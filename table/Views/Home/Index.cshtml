@{
    ViewData["Title"] = "Home Page";
}



<table class="table table-bordered">
    <tr>

        <th>Name</th>
        <th>Location</th>

    </tr>
    @foreach(var item in ViewBag.contacts)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Location</td>
        </tr>
    }
</table>
